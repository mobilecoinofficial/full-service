{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "feature/renovate-testing"
  ],
  "enabledManagers": [
    "git-submodules"
  ],
  "cloneSubmodules": true,
  "git-submodules": {
    "enabled": true,
    "versioning": "git",
    "fileMatch": [
      "(^|/)\\.gitmodules$"
    ]
  }
}